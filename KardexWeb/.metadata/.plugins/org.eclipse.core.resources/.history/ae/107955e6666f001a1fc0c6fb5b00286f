package model;

public class Producto {
	int id;
	String name;
	String description;
	double price;
	int quantity;
	
	public Producto() {
	}
	
	public Producto(String name, String description, double price, int quantity) {
		this.name = name;
		this.description = description;
		this.price = price;
		this.quantity = quantity;
	}
}
